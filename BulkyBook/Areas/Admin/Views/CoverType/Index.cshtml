
@{ 
    ViewData["Title"] = "Cover Type";
    Layout = "~/Views/Shared/_Layout.cshtml"; 
}
<br />
<div class="row">
    <div class="col-6">
        <h2 class="text-primary">Cover Type List </h2>
    </div>
    <div class="col-6 text-end">
        <a class="btn btn-primary" asp-action="Upsert">Create New Cover Type</a>
    </div>
</div>

<br />
<div class="p-4 border rounded">
    <table id="categoryDataTable" class="table table-striped table-bordered " style="width:100%">
        <thead class="thead-primary ">
            <tr class="table-info">
                <th>Name</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>
@section Scripts
{
    <script src="~/js/coverType.js"></script>
}
